 code_elements :
  internal_properties : CAAAAAgAAAA43TXOxtV40AdLwtSUrnrPcIb7bsGkPEQvokqHQWcHCocZ3vaM/iDXhCqZZNwfTZ+CmKbuUj0/v8rZL237WSzuJ/ig5gnToqt60g==
