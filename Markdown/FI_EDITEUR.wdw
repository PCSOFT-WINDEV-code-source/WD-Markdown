   -
     name : VérifieSauvegardeSiModifié
     procedure_id : 1426178886065678904
     type_code : 14
     code : |1-
      // Résumé : Vérifie si on doit sauvegarder
      // Syntaxe :
      //[ <Résultat> = ] VérifieSauvegardeSiModifié ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : Vrai si on peut continuer, Faux sinon on doit arrêter
      //
      procédure privee VérifieSauvegardeSiModifié()
      
      	SI __MaFenInterne.AutoriseSauvegarde ALORS
      		SI SAI_MARKDOWN.Modifié ALORS
      			SELON Dialogue( "Les modifications actuelles n'ont pas été enregistrées. Souhaitez-vous les enregistrer maintenant ?", [ "Enregistrer", "Ne pas enregistrer", "Annuler" ] , 1, 3 )
      				CAS 1
      					
      					// Enregistre
      					SI Enregistre( ( SAI_MARKDOWN.Extra.FichierEnCours..Existe ? SAI_MARKDOWN.Extra.FichierEnCours SINON "" ) ) = Faux ALORS
      						renvoyer faux
      					FIN
      					
      				CAS 3
      					RENVOYER Faux
      			FIN
      		FIN
      	FIN
      	
      RENVOYER vrai
     type : 458752
   -
     name : __ChangeTaille
     procedure_id : 1427632827110842776
     type_code : 14
     code : |1-
      // Résumé : Exécutée lors du changement de taille
      // Syntaxe :
      // __ChangeTaille ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure privee __ChangeTaille()
      
      L est un reel
      SI __MaFenInterne.AperçuDisponible ALORS
      	L = ( DISP_MARKDOWN.Largeur - DISP_MARKDOWN.MargeInterCellules ) / 2
      	
      	DISP_MARKDOWN[ 1 ].Largeur		= L
      	SAI_MARKDOWN.Largeur			= L
      	DISP_MARKDOWN[ 2 ].Largeur		= L
      	SC_MARKDOWN.Largeur				= L
      SINON
      	
      	L = DISP_MARKDOWN.Largeur
      	DISP_MARKDOWN[ 1 ].Largeur	= L
      	SAI_MARKDOWN.Largeur		= L	
      FIN
     type : 458752
