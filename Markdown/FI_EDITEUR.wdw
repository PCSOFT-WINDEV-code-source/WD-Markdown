   -
     name : SélectionApplique
     internal_properties : CAAAAAgAAABGCUoGIgiOtGFk6S0rQaG3TS9SNUBE0jiihBLYHoLaytugvhnHW+Wma38Bg1GsqOZ8zkMPJCM0X4gXxtqSs99JqqSqFFg5mskFgau6MEyUNK/FNAhFhefbF61VKlFT4bh3VC+Mzhwy4HTc260WQYMaMD9FXzQnkY0CGgBR5t9zVVDW
     procedure_id : 1402767427446309477
     type_code : 14
     code : |1-
      // Résumé : Applique un 'style' markdown à la sélection courante
      // Syntaxe :
      //SélectionApplique (<eStyle> est FI_EDITEUR.FI_EDITEUR.StyleMarkdown [, <sOptions> est chaîne])
      //
      // Paramètres :
      //	eStyle (FI_EDITEUR.FI_EDITEUR.StyleMarkdown) : Code markdown avant
      //	sOptions (chaîne ANSI - valeur par défaut="") : Options du style
      // Valeur de retour :
      // 	Aucune
      //
      procédure privee SélectionApplique( local eStyle est un StyleMarkdown, local sOptions est une chaine = "" )
      
      	sAvant, sAprès						sont des chaînes		
      	nCurseur, nCurseurFin				sont des entiers		= ( SAI_MARKDOWN.Curseur, SAI_MARKDOWN.FinCurseur )
      	nMemCurseur, nMemCurseurFin			sont des entiers		= ( nCurseur, nCurseurFin )
      	nDiffCurseur, nDiffFinCurseur		sont des entiers
      	nDiffCurseurSel, nDiffFinCurseurSel	sont des entiers
      	( sAvant, sAprès )											= StyleVersMarkdown( ( eStyle ) )
      	bAvantRegex							est un booléen			= sAvant [=] "{0}"
      	bAprèsRegex							est un booléen			= sAprès [=] "{0}"
      
      	// Etend le texte sélectionné
      	sTexteSélectionné est une chaine = SélectionEtend( nCurseur, nCurseurFin, eStyle )
      	
      	// On a réduit la sélection
      	si nCurseur > nMemCurseur ALORS
      		nDiffCurseurSel = nCurseur - nMemCurseur
      	FIN
      	
      	// On a réduit la sélection
      	si nCurseurFin < nMemCurseurFin ALORS
      		nDiffFinCurseurSel = nCurseurFin - nMemCurseurFin
      	FIN
      	
      	// Aucun texte sélectionné : on n'applique pas de style de texte, seulement des styles de paragraphe
      	si sTexteSélectionné = "" ALORS
      		si eStyle dans ( gras, Italique, Gras_Italique, Barré, Couleur ) ALORS
      			retour
      		FIN
      		
      		si eStyle = image
      			// Il faut ajouter du texte pour que l'image apparaisse		
      			sTexteSélectionné = "Libellé"
      			nDiffFinCurseurSel += Taille( sTexteSélectionné )
      		
      		sinon si eStyle = lien ALORS
      			// Il faut ajouter du texte : on reprend l'url
      			sTexteSélectionné = URLExtraitChemin( sOptions, urlProtocole + urlDomaine )
      			nDiffFinCurseurSel += taille( sTexteSélectionné )
      		FIN
      	FIN
      	
      	// Si on applique un token de début, doit vérifier qu'il n'est pas déjà présent
      	SI eStyle = Titre_Aucun _OU_ ( sAvant <> "" _ET_ sAprès = "" ) ALORS
      		
      		// On applique un style de ligne 'Titre'
      		SI eStyle.nom [= "Titre_" ALORS
      			
      			// On va énumérer les styles de titre du plus 'grand' au plus 'petit'
      			POUR TOUT eStyleTitre DE [ Titre_6, Titre_5, Titre_4, Titre_3, Titre_2, Titre_1, Citation ]
      				
      				sAvantEx est une chaîne =  StyleVersMarkdown( ( eStyleTitre ) )
      				
      				// Le texte possède déjà le style appliqué
      				SI CompareAvant( sTexteSélectionné, sAvantEx ) ALORS
      					
      					// Mémorise la taille du texte sélectionné
      					nDiffCurseur		= sTexteSélectionné.Taille
      					
      					// Remplace la portion 'avant' par une chaîne vide
      					sTexteSélectionné	= Remplace( sTexteSélectionné, sAvantEx, "" )	
      					
      					// Soustrait la nouvelle taille du texte sélectionné
      					nDiffCurseur -= sTexteSélectionné.Taille
      					
      					// On inverse
      					nDiffCurseur = -nDiffCurseur				
      					SORTIR
      				FIN
      			FIN
      			
      		SINON SI eStyle = Citation ALORS
      			
      			// Mémorise la taille de la sélection
      			nTailleAvant	est un entier	= sTexteSélectionné.Taille		
      			
      			// Pour chaque ligne sélectionnée			
      			tabSélection	est un tableau de chaînes
      			ChaîneVersTableau( sTexteSélectionné, tabSélection, RC )
      			bTrouvé est un booléen = Faux	
      			
      			POUR TOUT sTexteLigneSélection, nLigne de tabSélection
      				
      				// Le texte possède déjà le style appliqué
      				SI CompareAvant( sTexteLigneSélection, sAvant ) ALORS
      					
      					// Remplace la portion 'avant' par une chaîne vide
      					sTexteLigneSélection[ 1 SUR sAvant.Taille ]	= ""				
      					bTrouvé										= Vrai		
      				FIN
      			FIN
      			
      			// On ne remet rien à la place si on a trouvé
      			SI bTrouvé ALORS sAvant = ""
      			
      			// Applique avant
      			nNumLigne est un entier = 1
      			tabSélection.Applique( sTexteLigneSélection => {
      				sTexteLigneSélection = Remplace( sAvant, "{0}", nNumLigne++ ) + sTexteLigneSélection
      			} )
      			
      			// Reconstruit la sélection
      			tabSélection		= tabSélection.Filtre( sTexteLigneSélection => sTexteLigneSélection <> "" )
      			sTexteSélectionné	= tabSélection.VersChaîne( bTrouvé ? RC SINON RC + RC )
      			
      			// On devra déplacer le curseur à droite d'autant
      			SI nMemCurseur = nMemCurseurFin ALORS
      				nDiffCurseur	= -( nTailleAvant - sTexteSélectionné.Taille )
      				nDiffFinCurseur	= nDiffCurseur
      			SINON
      				nDiffCurseur	= 0
      				nDiffFinCurseur	= -( nTailleAvant - sTexteSélectionné.Taille )
      			FIN
      			
      			// Modifie la sélection courante
      			SélectionModifie( sTexteSélectionné, nCurseur, nCurseurFin, nDiffCurseur + nDiffCurseurSel, nDiffFinCurseur + nDiffFinCurseurSel )
      			RETOUR
      			
      			// On applique un style de ligne 'Liste'
      		SINON SI eStyle.Nom [= "Liste"
      			
      			// Mémorise la taille de la sélection
      			nTailleAvant	est un entier	= sTexteSélectionné.Taille
      			
      			// Pour chaque ligne sélectionnée			
      			tabSélection	est un tableau de chaînes
      			ChaîneVersTableau( sTexteSélectionné, tabSélection, RC )
      			
      			// On va énumérer les styles de liste pour échanger
      			POUR TOUT eStyleTitre DE [ Liste2, Liste, ListeOrdonnée, Titre_6, Titre_5, Titre_4, Titre_3, Titre_2, Titre_1, Citation ]
      				
      				// Si on est sur le style à appliquer, on l'ignore
      				SI eStyleTitre = eStyle ALORS CONTINUER
      				
      				sAvantEx est une chaîne =  StyleVersMarkdown( ( eStyleTitre ) )
      				
      				POUR TOUT sTexteLigneSélection, nLigne de tabSélection
      					
      					// Le texte possède déjà le style appliqué
      					SI CompareAvant( sTexteLigneSélection, sAvantEx ) ALORS
      						
      						// Remplace la portion 'avant' par une chaîne vide
      						sTexteLigneSélection[ 1 SUR Taille( sAvantEx.Remplace( "{0}", nLigne ) ) ] = ""						
      					FIN
      				FIN
      			FIN
      			
      			// Recompose la sélection			
      			nLigne	est un entier			
      			bTrouvé	est un booléen	= Faux	
      			
      			POUR TOUT sTexteLigneSélection, nLigne de tabSélection						
      				
      				// Si le texte commence déjà par le style de liste demandé
      				SI CompareAvant( sTexteLigneSélection,  sAvant ) ALORS
      					
      					bTrouvé = Vrai				
      					
      					// Remplace la portion 'avant' par une chaîne vide
      					sTexteLigneSélection[ 1 SUR Taille( sAvant.Remplace( "{0}", nLigne ) ) ]	= ""					
      				FIN				
      			FIN
      			
      			// On ne remet rien à la place si on a trouvé
      			SI bTrouvé ALORS sAvant = ""
      			
      			// Applique avant
      			nNumLigne est un entier = 1
      			tabSélection.Applique( sTexteLigneSélection => {
      				sTexteLigneSélection = Remplace( sAvant, "{0}", nNumLigne++ ) + sTexteLigneSélection
      			} )
      			
      			// Reconstruit la sélection
      			sTexteSélectionné = tabSélection.VersChaîne( RC )			
      			
      			// On devra déplacer le curseur à droite d'autant
      			SI nMemCurseur = nMemCurseurFin ALORS
      				nDiffCurseur	= -( nTailleAvant - sTexteSélectionné.Taille )
      				nDiffFinCurseur	= nDiffCurseur
      			SINON
      				nDiffCurseur	= 0
      				nDiffFinCurseur	= -( nTailleAvant - sTexteSélectionné.Taille )
      			FIN
      			
      			// Modifie la sélection courante
      			SélectionModifie( sTexteSélectionné, nCurseur, nCurseurFin, nDiffCurseur + nDiffCurseurSel, nDiffFinCurseur + nDiffFinCurseurSel )
      			RETOUR
      			
      			// Cas particulier de la tabulation
      		SINON SI eStyle DANS ( Tabulation, TabulationShift ) ALORS			
      			
      			// Mémorise la taille de la sélection
      			nTailleAvant	est un entier	= sTexteSélectionné.Taille
      			
      			// Pour chaque ligne sélectionnée			
      			tabSélection	est un tableau de chaînes
      			ChaîneVersTableau( sTexteSélectionné, tabSélection, RC )
      						
      			// Applique avant
      			tabSélection.Applique( sTexteLigneSélection => {
      			
      				// Annulation de la tabulation
      				SI eStyle = TabulationShift ALORS
      					SI sTexteLigneSélection [= sAvant ALORS
      						sTexteLigneSélection[ 1 SUR sAvant.Taille ] = ""
      					FIN
      					
      					// Application de la tabulation				
      				SINON	
      					sTexteLigneSélection = sAvant + sTexteLigneSélection 
      				FIN			
      				
      			} )
      			
      			// Reconstruit la sélection
      			sTexteSélectionné	= tabSélection.VersChaîne( RC )			
      			
      			// On devra déplacer le curseur à droite d'autant
      			nDiffCurseur		= -( nTailleAvant - sTexteSélectionné.Taille )
      			nDiffFinCurseur		= nDiffCurseur
      			
      			// Modifie la sélection courante
      			SélectionModifie( sTexteSélectionné, nCurseur, nCurseurFin, nDiffCurseur + nDiffCurseurSel, nDiffFinCurseur + nDiffFinCurseurSel )
      			RETOUR
      		FIN		
      		
      		sTexteSélectionné	= sAvant + sTexteSélectionné	
      		
      		// On devra déplacer le curseur à droite d'autant
      		nDiffCurseur		= sAvant.Taille + nDiffCurseur
      		nDiffFinCurseur	+= nDiffCurseur			
      		
      		// Modifie la sélection courante
      		SélectionModifie( sTexteSélectionné, nCurseur, nCurseurFin, nDiffCurseur + nDiffCurseurSel, nDiffFinCurseur + nDiffFinCurseurSel )
      		
      	// Si on applique les mêmes tokens avant et après, on devra vérifier les paires avant/après
      	SINON SI sAvant <> "" _ET_ sAprès <> "" ALORS
      		
      		// Le texte possède déjà le style appliqué
      		SI CompareAvant( sTexteSélectionné )  _ET_ CompareAprès( sTexteSélectionné ) ALORS
      					
      			// Avec option sur la partie 'avant'
      			SI bAvantRegex ALORS
      				
      				// Si c'est la même option : supprime le style, sinon on remplace
      				SI sTexteSélectionné [= sAvant.Remplace( "{0}", sOptions ) ALORS
      					
      					// Mémorise la taille du texte sélectionné
      					nDiffCurseur		= sTexteSélectionné.Taille
      					
      					// Remplace la portion 'avant' par une chaîne vide
      					sTexteSélectionné	= RegexRemplace( sTexteSélectionné, VersRegex( sAvant, *, Faux ), "" )				
      					
      					// Soustrait la nouvelle taille du texte sélectionné
      					nDiffCurseur -= sTexteSélectionné.Taille
      					
      					// On inverse
      					nDiffCurseur = -nDiffCurseur				
      					
      				SINON
      					
      					// Remplace la portion 'avant' par une chaîne vide
      					sTexteSélectionné = RegexRemplace( sTexteSélectionné, VersRegex( sAvant, *, Faux ), sAvant.Remplace( "{0}", sOptions )  )								
      					
      					// Modifie la sélection courante
      					SélectionModifie( sTexteSélectionné, nCurseur, nCurseurFin, nDiffCurseur + nDiffCurseurSel, nDiffFinCurseur + nDiffFinCurseurSel )
      					RETOUR
      				FIN			
      				
      				// Avec option sur la partie 'après'
      			SINON SI bAprèsRegex ALORS
      				
      				// Si c'est la même option : supprime le style, sinon on remplace
      				SI sTexteSélectionné =] sAprès.Remplace( "{0}", sOptions ) ALORS
      					
      					// Remplace la portion 'après' par une chaîne vide
      					sTexteSélectionné = RegexRemplace( sTexteSélectionné, VersRegex( sAprès, Vrai, Faux ), "" )				
      					
      				SINON
      					
      					// Remplace la portion 'après' par une chaîne vide
      					sTexteSélectionné = RegexRemplace( sTexteSélectionné, VersRegex( sAprès, Vrai, Faux ), sAprès.Remplace( "{0}", sOptions ) )								
      					
      					// Modifie la sélection courante
      					SélectionModifie( sTexteSélectionné, nCurseur, nCurseurFin, nDiffCurseur + nDiffCurseurSel, nDiffFinCurseur + nDiffFinCurseurSel )
      					RETOUR
      				FIN
      				
      			SINON
      				
      				// Supprime la partie avant
      				sTexteSélectionné[ À sAvant.Taille ]	= ""
      				
      				// On devra déplacer le curseur à gauche d'autant
      				nDiffCurseur							= -sAvant.Taille
      			FIN
      			
      			// On remplace 'après'
      			SI bAprèsRegex ALORS
      				
      				SI nMemCurseur = nCurseur ALORS
      					nDiffCurseur	= 0
      					nDiffFinCurseur	= -( sAvant.Taille + sAprès.Taille )
      				SINON
      					nDiffCurseur	= -sAvant.Taille
      					nDiffFinCurseur	= nDiffCurseur
      				FIN
      				
      				// Supprime la partie avant
      				sTexteSélectionné[ À sAvant.Taille ]	= ""
      				
      			// On remplace 'avant'
      			SINON	
      				
      				SI nMemCurseur = nCurseur ALORS
      					nDiffCurseur	= 0
      					nDiffFinCurseur	= -( sAvant.Taille + sAprès.Taille )
      				SINON
      					nDiffFinCurseur = nDiffCurseur
      				FIN
      				
      				// Supprime la partie 'après' du texte sélectionné
      				sTexteSélectionné[ sTexteSélectionné.Taille - sAprès.Taille + 1 À ] = ""
      			FIN
      		SINON
      			
      			// Complète la partie avant avec les options fournies
      			sAvant				= sAvant.Remplace( "{0}", sOptions )
      			sAprès				= sAprès.Remplace( "{0}", sOptions )
      			
      			// Applique le style			
      			sTexteSélectionné	= sAvant + sTexteSélectionné + sAprès
      			
      			// On devra déplacer le curseur à droite d'autant
      			nDiffCurseur 	+= sAvant.Taille
      			nDiffFinCurseur = nDiffCurseur
      		FIN
      		
      		// Modifie la sélection courante
      		SélectionModifie( sTexteSélectionné, nCurseur, nCurseurFin, nDiffCurseur + nDiffCurseurSel, nDiffFinCurseur + nDiffFinCurseurSel )
      	FIN
      	
      		PROCÉDURE INTERNE CompareAvant( sTexte est une chaîne, sTexteAvant est une chaîne = sAvant )
      			SI bAvantRegex ALORS
      				RENVOYER RegexCherche( sTexte, VersRegex( sTexteAvant ) + "[^[:space:][:punct:]]", [] ) > 0
      			SINON
      				RENVOYER sTexte [= sTexteAvant
      			FIN
      		FIN	
      	
      		PROCÉDURE INTERNE CompareAprès( sTexte est une chaîne, sTexteAprès est une chaîne = sAprès )
      			SI bAprèsRegex ALORS
      				RENVOYER RegexCherche( sTexte, VersRegex( sTexteAprès, Vrai ), [] ) > 0
      			SINON
      				RENVOYER sTexte =] sTexteAprès
      			FIN
      		FIN	
     type : 458752
   -
     name : SélectionEtend
     internal_properties : CAAAAAgAAADCrDvCNlsnDTI3JpEQAj1E1a5vL2jx1lUq2jD6y4BVILJulKlDApyHKucAvmee7pos+anHgMN8WS88IodgdVObied/bUWn3FL4KLZiy8kGZWzp0lXu9WM3mQRg1oNxtXKHC9G/aXD8/SmJNBjaXA3oeqVNSHatDHzK46dsmkGpTs+MiM4LpyUo/5xdUJ6APGkqAk8tSx+5oT2gUm8TxkYBZRUsdVc7u2Mp5qn7IHbyF2cMy9phAl3V7zO4T+d0oo9cSxW/Qqxm2VfFQQ==
     procedure_id : 1399758587988002930
     type_code : 14
     code : |1-
      // Résumé : Etend virtuellement la sélection courante pour appliquer le style de façon intelligente 
      // Syntaxe :
      //[ <Résultat> = ] SélectionEtend (<nCurseur> est entier, <nCurseurFin> est entier, <eStyle> est FI_EDITEUR.FI_EDITEUR.StyleMarkdown)
      //
      // Paramètres :
      //	nCurseur (entier) : Position du curseur (pourra être modifié)
      //	nCurseurFin (entier) : Position de fin du curseur (pourra être modifié)
      //	eStyle (FI_EDITEUR.FI_EDITEUR.StyleMarkdown) : Style à appliquer
      
      // Valeur de retour :
      // 	chaîne ANSI : Texte sélectionné virtuellement
      //
      procédure privee SélectionEtend( nCurseur est un entier, nCurseurFin est un entier, local eStyle est un StyleMarkdown ) : chaîne
      
      	sSélection	est une chaine	= SAI_MARKDOWN.TexteSélectionné
      	sTexteAvant	est une chaine	= SAI_MARKDOWN[[ 1 a nCurseur - 1 ]]
      	sTexteAprès	est une chaine	= SAI_MARKDOWN[[ nCurseurFin A ]]
      	
      	///////////////////////////////////////
      	// Corrections
      	///////////////////////////////////////
      		
      	// Sélection de la couleur
      	SI RegexVérifie( "<span " + sSélection, "<span style=""color:[^""]+"">.*<\/span>" ) ALORS
      		
      		// Mise à jour de la sélection
      		nCurseur -= 6		
      		sTexteAvant	= SAI_MARKDOWN[[ 1 a nCurseur - 1 ]]
      		
      		// Modifie la sélection virtuelle
      		sSélection = SAI_MARKDOWN[[ nCurseur a nCurseurFin - 1 ]]
      	FIN
      	
      	// Cas des liens et des images
      	si ( sSélection [= "[" _et_ sSélection =] "]" )  alors
      
      		SI RegexVérifie( sTexteAprès, "\]?\(.*\).*" ) ALORS	
      
      			// C'est une image
      			SI sTexteAvant =] "!" _OU_ sTexteAvant =] "![" ALORS				
      				
      				// On doit étendre la sélection
      				nCurseur --
      				nCurseurFin += sTexteAprès.position( ")" ) - 1
      				
      			sinon
      
      				// On doit contracter la sélection
      				nCurseur ++					
      				nCurseurFin --						
      			fin
      						
      			// Modifie le texte avant et après
      			sTexteAvant	= SAI_MARKDOWN[[ 1 À nCurseur - 1 ]]					
      			sTexteAprès	= SAI_MARKDOWN[[ nCurseurFin À ]]				
      			
      			// Modifie la sélection virtuelle
      			sSélection	= SAI_MARKDOWN[[ nCurseur À nCurseurFin - 1 ]]				
      			
      		FIN
      	
      	sinon si ( sTexteAvant =] "[" _et_ sTexteAprès [= "]" ) ALORS	
      
      		SI RegexVérifie( sTexteAprès, "\]?\(.*\).*" ) ALORS
      						
      			// C'est une image
      			SI sTexteAvant =] "!" _OU_ sTexteAvant =] "![" ALORS				
      				
      				// On doit étendre la sélection
      				SI sTexteAvant =] "!" alors nCurseur --
      				SI sTexteAvant =] "![" alors nCurseur -= 2
      				
      				nCurseurFin += sTexteAprès.Position( ")" )
      			fin			
      						
      			// Modifie le texte avant et après
      			sTexteAvant	= SAI_MARKDOWN[[ 1 À nCurseur - 1 ]]					
      			sTexteAprès = SAI_MARKDOWN[[ nCurseurFin A ]]				
      			
      			// Modifie la sélection virtuelle
      			sSélection = SAI_MARKDOWN[[ nCurseur À nCurseurFin - 1 ]]		
      		FIN		
      	FIN	
      
      	// Vérifie tous les styles possibles
      	selon eStyle
      	
      		// Lignes
      		CAS Titre_1, Titre_2, Titre_3, Titre_4, Titre_5, Titre_6, Titre_Aucun
      			
      			// On revient au début de la ligne
      			nPosFin   est un entier = min( nCurseur, taille( SAI_MARKDOWN ) )
      			nPosDébut est un entier = max( 1, position( SAI_MARKDOWN, rc, nPosFin - 1, depuisfin ) )		
      			
      			SI nPosDébut > nPosFin ALORS
      				nPosDébut <=> nPosFin
      			FIN
      			
      			// Elimine le retour chariot
      			sTexteAvant = SAI_MARKDOWN[[ nPosDébut a ]]
      			tantque sTexteAvant[ 1 ] dans ( Caract( 10 ), Caract( 13 ) )
      				nPosDébut ++
      				sTexteAvant = SAI_MARKDOWN[[ nPosDébut À ]]
      			FIN
      			
      			// Modifie le curseur virtuel
      			nCurseur = nPosDébut
      			nCurseurFin = position( SAI_MARKDOWN, rc, nPosDébut )			
      			si nCurseurFin < 1 alors nCurseurFin = taille( SAI_MARKDOWN ) + 1
      			
      			// Mise à jour
      			sSélection = SAI_MARKDOWN[[ nCurseur À nCurseurFin - 1 ]]
      			
      		// Paragraphes
      		cas Liste, Liste2, ListeOrdonnée, citation
      					
      			// On revient au début de la ligne
      			nPosFin   est un entier = min( nCurseur, taille( SAI_MARKDOWN ) )
      			nPosDébut est un entier = max( 1, position( SAI_MARKDOWN, rc, nPosFin - 1, depuisfin ) )		
      			
      			si nPosDébut > nPosFin ALORS
      				nPosDébut <=> nPosFin
      			FIN
      			
      			// Elimine le retour chariot
      			sTexteAvant = SAI_MARKDOWN[[ nPosDébut À ]]
      			TANTQUE sTexteAvant[ 1 ] DANS ( Caract( 10 ), Caract( 13 ), tab, " " )
      				nPosDébut ++
      				sTexteAvant = SAI_MARKDOWN[[ nPosDébut À ]]
      			FIN
      			
      			// Modifie le curseur virtuel
      			nCurseur	= nPosDébut
      			nCurseurFin	= Position( SAI_MARKDOWN, rc, nPosDébut + taille( sSélection ) - 1 )			
      			SI nCurseurFin < 1 ALORS nCurseurFin = Taille( SAI_MARKDOWN ) + 1
      			
      			// Mise à jour
      			sSélection = SAI_MARKDOWN[[ nCurseur À nCurseurFin - 1 ]]		
      			
      		// Paragraphes
      		cas Tabulation, TabulationShift
      					
      			// On revient au début de la ligne
      			nPosFin   est un entier = min( nCurseur, taille( SAI_MARKDOWN ) )
      			nPosDébut est un entier = max( 1, position( SAI_MARKDOWN, rc, nPosFin - 1, depuisfin ) )		
      			
      			// Elimine le retour chariot
      			sTexteAvant = SAI_MARKDOWN[[ nPosDébut À ]]
      			TANTQUE sTexteAvant[ 1 ] DANS ( Caract( 10 ), Caract( 13 ) )
      				nPosDébut ++
      				sTexteAvant = SAI_MARKDOWN[[ nPosDébut À ]]
      			FIN
      			
      			// Modifie le curseur virtuel
      			nCurseur	= nPosDébut
      			nCurseurFin	= Position( SAI_MARKDOWN, rc, nPosDébut + taille( sSélection ) - 1 )			
      			SI nCurseurFin < 1 ALORS nCurseurFin = Taille( SAI_MARKDOWN ) + 1
      			
      			// Mise à jour
      			sSélection = SAI_MARKDOWN[[ nCurseur À nCurseurFin - 1 ]]		
      					
      		// Paires			
      		CAS Gras, Italique, Gras_Italique, Barré, Couleur, image, lien	
      			
      			// Etape 1 : si aucun texte n'est sélectionné (cas d'un curseur positionné dans un mot par ex.)
      			// On étend automatiquement la sélection au mot dans lequel le curseur se trouve
      			si sSélection = "" ALORS
      				
      				nEspAvant, nEspAprès sont des entiers
      				tabEsp est un tableau de chaines = [ " ", "*", ">", "<", "[", "]", "(", ")" ]
      				si eStyle dans (image, lien) alors tabEsp.ajoute( rc )
      				
      				nEspAvant = sTexteAvant.Position( tabEsp, sTexteAvant.Taille, depuisfin )
      				nEspAprès = sTexteAprès.Position( tabEsp )
      				
      				si nEspAvant < 1 alors nEspAvant = 0
      				si nEspAprès < 1 alors nEspAprès = sTexteAprès.Position( rc, nCurseurFin )
      				si nEspAprès < 1 alors nEspAprès = sTexteAprès.Taille + 1
      				
      				// Mise à jour de la sélection virtuelle
      				nCurseur = nEspAvant + 1
      				nCurseurFin = nCurseurFin + ( nEspAprès - 1 )
      				
      				// Nouveau texte sélectionné
      				sSélection = SAI_MARKDOWN[[ nCurseur a nCurseurFin - 1 ]]
      				tantque sSélection [= rc 
      					nCurseur += 2
      					sSélection = SAI_MARKDOWN[[ nCurseur a nCurseurFin - 1 ]]
      				FIN
      				
      				// Mise à jour des parties avant/après
      				sTexteAvant	= SAI_MARKDOWN[[ 1 a nCurseur - 1 ]]
      				sTexteAprès	= SAI_MARKDOWN[[ nCurseurFin A ]]
      			FIN
      			
      			// Etape 2 : on élimine les espaces
      			tantque sSélection [= " "
      				
      				nCurseur ++
      				
      				// Nouveau texte sélectionné
      				sSélection	= SAI_MARKDOWN[[ nCurseur À nCurseurFin - 1 ]]
      				
      				// Mise à jour des parties avant/après
      				sTexteAvant	= SAI_MARKDOWN[[ 1 À nCurseur - 1 ]]
      				sTexteAprès	= SAI_MARKDOWN[[ nCurseurFin À ]]
      			FIN
      			
      			tantque sSélection =] " "
      				
      				nCurseurFin --
      				
      				// Nouveau texte sélectionné
      				sSélection	= SAI_MARKDOWN[[ nCurseur À nCurseurFin - 1 ]]
      				
      				// Mise à jour des parties avant/après
      				sTexteAvant	= SAI_MARKDOWN[[ 1 À nCurseur - 1 ]]
      				sTexteAprès	= SAI_MARKDOWN[[ nCurseurFin À ]]
      			FIN
      				
      			tabOccurrences est un tableau de chaines		
      			sAvant, sAprès sont des chaines	
      					
      			// Si on applique l'italique
      			si eStyle = Italique ALORS
      				
      				// On vérifie d'abord si on trouve un 'Gras+Italique'
      				si __AnalyseAvant( nCurseur, Gras_Italique ) = faux ALORS
      					
      					// Sinon on vérifie ensuite si on trouve un 'Italique'					
      					__AnalyseAvant( nCurseur, Italique )
      					__AnalyseAprès( nCurseurFin, Italique )
      				sinon
      					__AnalyseAprès( nCurseurFin, Gras_Italique )
      				FIN
      			sinon 
      				
      				si sSélection ~= "" _ou_ sSélection dans ( Rc, caract( 10 ) ) ALORS
      					sSélection = ""
      				sinon
      					
      					// Vérifie si on trouve le <style>
      					__AnalyseAvant( nCurseur, eStyle )
      					__AnalyseAprès( nCurseurFin, eStyle )			
      				FIN
      			FIN
      		
      			// Analyse le texte avant le curseur pour vérifier si on doit appliquer ou supprimer le style
      			procedure interne __AnalyseAvant( nCurseurEx est un entier, eStyleEx est un StyleMarkdown )		
      			
      				( sAvant, sAprès ) = StyleVersMarkdown( eStyleEx )				
      				bAvantRegex est un booléen = sAvant [=] "{0}"
      				bAprèsRegex est un booléen = sAprès [=] "{0}"
      				sExclusion est une chaine = CalculeExclusion()
      								
      				// Compte le nombre de <avant>*<après>
      				RegexCherche( sTexteAvant, VersRegex( sAvant ) + sExclusion + VersRegex( sAprès, vrai ), tabOccurrences )
      				
      				// Supprime les occurrences
      				sTexteAvant = sTexteAvant.Remplace( tabOccurrences, "" )
      				
      				// Si on a une occurrence de 'avant'
      				si ContientEx( sTexteAvant, bAvantRegex, sAvant, vrai ) ALORS
      					
      					// Mémorise le curseur de début actuel
      					nBackupCurseur est un entier = nCurseurEx				
      					
      					// Calcule la nouvelle position du curseur de début pour prendre en compte le 'avant'
      					soit N = Position( SAI_MARKDOWN, sAvant.ExtraitChaîne( 1, "{0}" ), nCurseurEx, DepuisFin )
      					
      					// Vérifie que l'on a a bien l'élément recherché
      					si RegexCherche( SAI_MARKDOWN[[ N a ]], VersRegex( sAvant ) + sExclusion, [] ) > 0 ALORS
      						nCurseurEx = N
      						
      						// Change le texte sélectionné
      						sSélection = SAI_MARKDOWN[[ nCurseurEx a nBackupCurseur - 1 ]] + sSélection
      						
      						renvoyer vrai
      					FIN
      				FIN			
      				
      				renvoyer faux
      			fin
      			
      			// Analyse le texte après le curseur pour vérifier si on doit appliquer ou supprimer le style
      			PROCÉDURE INTERNE __AnalyseAprès( nCurseurEx est un entier, eStyleEx est un StyleMarkdown )		
      				
      				( sAvant, sAprès ) = StyleVersMarkdown( eStyleEx )				
      				bAvantRegex	est un booléen	= sAvant [=] "{0}"
      				bAprèsRegex	est un booléen	= sAprès [=] "{0}"
      				sExclusion	est une chaine	= CalculeExclusion()
      				
      				// Compte le nombre de <avant>*<après>
      				RegexCherche( sTexteAprès, VersRegex( sAvant ) + sExclusion + VersRegex( sAprès, vrai ), tabOccurrences )
      				
      				// Supprime les occurrences
      				sTexteAprès = sTexteAprès.Remplace( tabOccurrences, "" )
      				
      				// Si on a une occurrence de 'avant'
      				SI ContientEx( sTexteAprès, bAprèsRegex, sAprès, faux ) ALORS
      					
      					// Mémorise le curseur de début actuel
      					nBackupCurseur est un entier = nCurseurEx			
      					
      					sAprèsEx est une chaine = sAprès.ExtraitChaîne( 2, "{0}" )	
      					si sAprèsEx = eot alors sAprèsEx = sAprès.ExtraitChaîne( 1, "{0}" )	
      					
      					// Calcule la nouvelle position du curseur de fin pour prendre en compte le 'après'
      					nCurseurEx	= Position( SAI_MARKDOWN, sAprèsEx, nCurseurEx ) + taille( sAprèsEx )
      					
      					// Change le texte sélectionné
      					sSélection	= sSélection + SAI_MARKDOWN[[ nBackupCurseur a nCurseurEx - 1 ]]
      										
      					RENVOYER Vrai
      				FIN			
      				
      				RENVOYER Faux
      			FIN
      			
      			// Génère une expression regex pour exclure des caractères
      			procedure interne CalculeExclusion()
      
      				renvoyer "(?:" + "(?!(" + VersRegex( sAvant, *, Faux ) + ".*" + VersRegex( sAprès, *, Faux ) + "))" + ".)"
      			FIN
      			
      			// Version avancée de la fonction Contient
      			PROCÉDURE INTERNE ContientEx( sTexte est une chaine, bRegEx est un booléen, sPartie est une chaine, bAvant est un booléen )
      				
      				sExclusion est une chaine = CalculeExclusion()
      				
      				// si on a une regex
      				SI bRegEx ALORS
      					
      					// vérifie si le texte contient la partie
      					SI bAvant ALORS
      						RENVOYER RegexCherche( sTexte, sExclusion + VersRegex( sPartie ), [] ) > 0						
      					sinon
      						RENVOYER RegexCherche( sTexte, VersRegex( sPartie ) + sExclusion, [] ) > 0
      					FIN
      				SINON
      					
      					// vérifie si le texte contient la partie
      					SI sTexte [=] sPartie ALORS
      						
      						si bAvant ALORS
      							
      							nPos est un entier = sTexte.Position( sPartie, sTexte.Taille, DepuisFin ) - sPartie.Taille
      							
      							// on vérifie que juste avant la partie, il n'y en a pas d'autres
      							SI sTexte[ nPos SUR sPartie.Taille ] <> sPartie ALORS
      								RENVOYER Vrai
      							FIN
      						sinon
      							// on vérifie que juste après la partie, il n'y en a pas d'autres
      							SI sTexte[ sTexte.Position( sPartie ) + sPartie.Taille SUR sPartie.Taille ] <> sPartie ALORS
      								RENVOYER Vrai
      							FIN
      						FIN
      					FIN
      				FIN
      				
      				RENVOYER Faux
      			FIN	
      			
      	FIN
      	
      	
      renvoyer sSélection
     type : 458752
   -
     name : SélectionModifie
     procedure_id : 1399759404034403815
     type_code : 14
     code : |1+
      // Résumé : Modifie le texte de la sélection
      // Syntaxe :
      //SélectionModifie (<sTexte> est chaîne, <nCurseur> est entier, <nCurseurFin> est entier, <nDiffCurseur> est entier, <nDiffFinCurseur> est entier)
      //
      // Paramètres :
      //	sTexte (chaîne ANSI) : Texte modifié
      //	nCurseur (entier) : Position de début
      //	nCurseurFin (entier) : Position de fin
      //	nDiffCurseur (entier) : Différence à appliquer au curseur de début
      //	nDiffFinCurseur (entier) : Différence à appliquer au curseur de fin
      // Valeur de retour :
      // 	Aucune
      //
      procédure privee SélectionModifie( sTexte est une chaine, nCurseur est un entier, nCurseurFin est un entier, nDiffCurseur est un entier,nDiffFinCurseur est un entier )
      
      	// Mémorise la sélection actuelle
      	nMemCurseur, nMemCurseurFin sont des entiers = ( SAI_MARKDOWN.Curseur, SAI_MARKDOWN.FinCurseur )
      
      	SAI_MARKDOWN.AffichageActif = faux
      	LIB_MARKDOWN.AffichageActif = faux
      	
      	// Cas particulier des <RC> qui peuvent être 'coupés'
      	// en deux soit à gauche, soit à droite
      	si (nCurseurFin - 1 ) - nCurseur <= 1 ALORS
      		si SAI_MARKDOWN[[ nCurseur À nCurseurFin ]] = rc ALORS
      			nCurseurFin++
      			sTexte = rc + sTexte
      		sinon SI SAI_MARKDOWN[[ nCurseur - 1 À nCurseurFin - 1 ]] = RC ALORS
      			nCurseur--
      			sTexte = RC + sTexte
      		FIN
      	FIN
      
      	// Mise à jour
      	SAI_MARKDOWN[[ nCurseur a nCurseurFin - 1 ]] = sTexte
      	
      	// Ré-applique la sélection mémorisée
      	SAI_MARKDOWN.Curseur = nMemCurseur + nDiffCurseur
      	SAI_MARKDOWN.finCurseur = nMemCurseurFin + nDiffFinCurseur
      	
      	AfficheMarkdown()	
      		
      	LIB_MARKDOWN.AffichageActif = vrai
      	SAI_MARKDOWN.AffichageActif = vrai
     type : 458752
   -
     name : StyleVersMarkdown
     procedure_id : 1399768311815820510
     type_code : 14
     code : |1-
      // Résumé : Convertit un style en texte
      // Syntaxe :
      //[ <Résultat> = ] StyleVersMarkdown (<eStyle> est FI_EDITEUR.FI_EDITEUR.StyleMarkdown)
      //
      // Paramètres :
      //	eStyle (FI_EDITEUR.FI_EDITEUR.StyleMarkdown) : Style
      // Valeur de retour :
      // 	multi-valeur : Texte avant, texte après
      //
      procédure privee StyleVersMarkdown( eStyle est un StyleMarkdown ) : ( chaine, chaine )
      
      	selon eStyle
      		CAS Gras
      			renvoyer ( "**", "**" )
      			
      		CAS Italique
      			renvoyer ( "*", "*" )
      			
      		CAS Gras_Italique
      			renvoyer ( "***", "***" )
      			
      		CAS Barré
      			renvoyer ( "~~", "~~" )			
      			
      		CAS Couleur
      			renvoyer ( "<span style=""color:{0}"">", "</span>" )	
      			
      		cas Lien
      			renvoyer ( "[", "]({0})")
      			
      		cas image
      			renvoyer ( "![", "]({0})")
      			
      		cas Liste
      			RENVOYER ( "- ", "" )
      			
      		cas Liste2
      			RENVOYER ( "* ", "" )
      		
      		cas ListeOrdonnée
      			RENVOYER ( "{0}. ", "" )
      			
      		cas Citation
      			RENVOYER ( "> ", "" )
      			
      		cas Tabulation
      			RENVOYER ( tab, "" )
      			
      		cas TabulationShift
      			RENVOYER ( tab, "" )
      			
      		CAS Titre_1
      			renvoyer ( "# ", "" )	
      			
      		CAS Titre_2
      			renvoyer ( "## ", "" )	
      			
      		CAS Titre_3
      			renvoyer ( "### ", "" )	
      			
      		CAS Titre_4
      			renvoyer ( "#### ", "" )	
      			
      		CAS Titre_5
      			renvoyer ( "##### ", "" )	
      			
      		CAS Titre_6
      			renvoyer ( "###### ", "" )	
      			
      		CAS Titre_Aucun
      			renvoyer ( "", "" )	
      			
      	FIN
      	
      	dbgAssertion( faux, "Style inconnu" )
      	
      renvoyer ( "", "" )
     type : 458752
   -
     name : VersRegex
     internal_properties : CAAAAAgAAABnUWatHZUXMoDb8Dy57Vr5+ic//F/Y/IEPvb78nKEJSVeikCR1rh3dNvUTGqCO8yTFCDhYZt7PA4euLU4nuVUtZNjRPJgRAUNpSz/1BQM/M7T3ZS6tif+TygtiXqVlQWvaRbK/vHrORgiqWwA=
     procedure_id : 1399801915685910257
     type_code : 14
     code : |1-
      // Résumé : Protège un texte pour un usage en regex
      // Syntaxe :
      //[ <Résultat> = ] VersRegex (<sTexte> est chaîne [, <bPrécéder> est booléen [, <bMotComplet> est booléen]])
      //
      // Paramètres :
      //	sTexte (chaîne ANSI) : Texte à protéger
      //	bPrécéder (booléen - valeur par défaut=0) : Vrai si quelque chose peut précéder ce texte (sans espace), Faux sinon
      // 	bMotComplet (booléen - valeur par défaut=1) : Vrai si le texte est un mot complet, Faux sinon
      
      // Valeur de retour :
      // 	chaîne ANSI : Texte avec les séquences protégées
      //
      procédure privée VersRegex( local sTexte est une chaine, bPrécéder est un booléen = faux, bMotComplet est un booléen = vrai ) : chaine
      
      	sTexte = sTexte.remplace( "\", "\\" )
      	sTexte = sTexte.remplace( "*", "\*" )
      	sTexte = sTexte.remplace( ".", "\." )
      	sTexte = sTexte.remplace( "/", "\/" )
      	sTexte = sTexte.remplace( "~", "\~" )
      	sTexte = sTexte.remplace( "!", "\!" )
      	sTexte = sTexte.remplace( "(", "\(" )
      	sTexte = sTexte.remplace( ")", "\)" )
      	sTexte = sTexte.remplace( "[", "\[" )
      	sTexte = sTexte.remplace( "]", "\]" )
      	sTexte = sTexte.remplace( "{0}", "[^""]+" )
      	
      	si bMotComplet ALORS
      		si sTexte pas =] " " ALORS
      			si bPrécéder ALORS
      				sTexte = "[^\s]*" + sTexte
      			sinon
      				sTexte += "[^\s]*"
      			FIN
      		FIN
      	FIN
      
      renvoyer sTexte
     type : 458752
   -
     name : __SurTabulation
     procedure_id : 1401290139117873323
     type_code : 14
     code : |1-
      // Résumé : Gère les tabulations
      // Syntaxe :
      // __SurTabulation ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure PRIVÉE __SurTabulation()
      	
      	// Si on a une sélection
      	SI SAI_MARKDOWN.TexteSélectionné <> "" ALORS
      		
      		SI ToucheEnfoncée( teShift ) ALORS
      			SélectionApplique( TabulationShift )
      		SINON	
      			SélectionApplique( Tabulation )
      		FIN
      		
      		RENVOYER Faux
      	FIN		
      	
      	// Si on est positionné au début d'une ligne ou juste avant une tabulation
      	SI SAI_MARKDOWN.Curseur = 1 _OU_ SAI_MARKDOWN[[ SAI_MARKDOWN.Curseur - 2 SUR 2 ]] = RC _OU_ ( SAI_MARKDOWN[[ SAI_MARKDOWN.Curseur - 1 SUR 1 ]] = TAB _OU_ SAI_MARKDOWN[[ SAI_MARKDOWN.Curseur - Taille( TAB_ESPACES ) SUR Taille( TAB_ESPACES ) ]] = TAB_ESPACES ) ALORS
      		
      		SI ToucheEnfoncée( teShift ) ALORS
      			SélectionApplique( TabulationShift )
      		SINON	
      			SélectionApplique( Tabulation )
      		FIN
      		
      		RENVOYER Faux
      	FIN
      	
      	// Si on a une déclaration d'une liste juste avant le caractère
      	SI SAI_MARKDOWN[[ SAI_MARKDOWN.Curseur - 2 SUR 2 ]] = "- " _OU_ SAI_MARKDOWN[[ SAI_MARKDOWN.Curseur - 2 SUR 2 ]] = "* " ALORS
      		
      		SI ToucheEnfoncée( teShift ) ALORS
      			SélectionApplique( TabulationShift )
      		SINON	
      			SélectionApplique( Tabulation )
      		FIN
      		
      		RENVOYER Faux
      	fin
      
      	// CTRL+TAB pour insérer une tabulation dans le texte	
      	si ToucheEnfoncée( teControl ) ALORS
      		renvoyer vrai
      	FIN
      	
      renvoyer faux
     type : 458752
   -
     name : Exporte
     internal_properties : CAAAAAgAAAAUv/hswTZwfp2CLVlRv0mv8TVWmfZS1j5TCGoQqgaGxkskTlkbh0EeeyuBQ53o/PIcKktXMP/oLzB7TgrOTyN1coLsYgoPxCePHzUwInpu+unDnh4nm8H50QsfYAt5s7ZpWgG6gFokNnbT3KYFgkIDIziQBJf8jOJx15v+wT2dA/4E
     procedure_id : 1401660799194022356
     type_code : 14
     group : 4
     code : |1-
      // Résumé : Exporte le markdown
      // Syntaxe :
      //Exporte (<nFormat> est entier)
      //
      // Paramètres :
      //	nFormat (entier) : Format d'export
      // Valeur de retour :
      // 	Aucune
      //
      procédure Exporte( nFormat est un entier )
      
      	si __MaFenInterne.AutoriseExport = faux ALORS
      		retour
      	FIN
      
      	sFichier est une chaîne
      
      	selon nFormat
      		CAS itexte
      					
      			// Ouvre le sélecteur de fichiers
      			sFichier = fSélecteur("", "", "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Fichiers texte (*.TXT)" + TAB + "*.TXT", "TXT", fselCrée + fselExiste)
      			si sFichier <> "" ALORS
      				fSauveTexte( sFichier, MarkdownVersTexte( SAI_MARKDOWN ) ) 
      			FIN
      				
      		CAS ihtml
      			
      			// Ouvre le sélecteur de fichiers
      			sFichier = fSélecteur("", "", "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Fichiers HTML (*.HTML,*.HTM)" + TAB + "*.HTML;*.HTM", "HTML", fselCrée + fselExiste)
      			SI sFichier <> "" ALORS
      				
      				sHTML est une chaine ansi = MarkdownVersHTML( SAI_MARKDOWN )
      				
      				// MarkdownVersHTML renvoie une chaîne au format UTF8
      				// Il faut préciser le format dans le HTML généré
      				sHTML = "<html><head><meta charset=""UTF-8""></head><body>" + sHTML + "</body></html>"
      				
      				fSauvebuffer( sFichier, sHTML ) 
      			FIN
      			
      		CAS iRTF
      			
      			// Ouvre le sélecteur de fichiers
      			sFichier = fSélecteur("", "", "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Documents Word (*.DOCX)" + TAB + "*.DOCX", "DOCX", fselCrée + fselExiste)
      			SI sFichier <> "" ALORS
      				
      				// Conversion
      				docExp est un document = MarkdownVersDOCX( SAI_MARKDOWN )
      				
      				// Sauvegarde
      				docExp.Sauve( sFichier ) 
      			FIN		
      			
      		CAS ipdf
      							
      			// Ouvre le sélecteur de fichiers
      			sFichier = fSélecteur("", "", "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Fichiers PDF (*.PDF)" + TAB + "*.PDF", "PDF", fselCrée + fselExiste)
      			SI sFichier <> "" ALORS
      				fSauveBuffer( sFichier, MarkdownVerspdf( SAI_MARKDOWN ) ) 
      			FIN
      	FIN
      	
      	si sFichier <> "" alors
      		LanceAppliAssociée( sFichier )
      	fin
      	
     type : 458752
   -
     name : AfficheMarkdown
     procedure_id : 1401666614606982391
     type_code : 14
     code : |1+
      // Résumé : Affiche le rendu du markdown
      // Syntaxe :
      // AfficheMarkdown ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure privee AfficheMarkdown()
      	
      	LIB_MARKDOWN				= SAI_MARKDOWN
      	
      	SC_MARKDOWN.LargeurUtile	= Max( SC_MARKDOWN.Largeur, TexteLargeur( LIB_MARKDOWN, SAI_MARKDOWN, tDéfaut ) )
      	SC_MARKDOWN.HauteurUtile	= Max( SC_MARKDOWN.Hauteur, TexteHauteur( LIB_MARKDOWN, SAI_MARKDOWN, tMultiligne + tAvecMarge )  )
      	
      	LIB_MARKDOWN.Largeur		= SC_MARKDOWN.LargeurUtile
      	LIB_MARKDOWN.Hauteur		= SC_MARKDOWN.HauteurUtile
     type : 458752
   -
     name : __AppuieTouche
     procedure_id : 1402749770736814701
     type_code : 14
     code : |1-
      // Résumé : Gère l'appui de touches spécifiques
      // Syntaxe :
      //[ <Résultat> = ] __AppuieTouche (<Touches> est tableau associatif (chaîne))
      //
      // Paramètres :
      //	Touches (tableau associatif (chaîne ANSI)) : Touches
      // Valeur de retour :
      // 	booléen : Vrai si la touche a été gérée, Faux sinon
      //
      procédure privee __AppuieTouche( Touches est un tableau associatif de chaines ) : booléen
      
      	nCurseur, nFinCurseur sont des entiers = ( SAI_MARKDOWN.Curseur, SAI_MARKDOWN.FinCurseur )
      	bLibellé est un booléen
      	
      	// Vérifie si on est dans le libellé d'une image ou le libellé d'un lien
      	sAvant est une chaine = SAI_MARKDOWN[[ a nCurseur - 1 ]]
      	sAprès est une chaine = SAI_MARKDOWN[[ nCurseur a ]]
      	
      	si position( sAvant, [ "[", "![" ], taille( sAvant ), depuisfin ) > Position( sAvant, "]", Taille( sAvant ), DepuisFin ) ALORS
      		si position( sAprès, [ "[", "![" ] ) = 0 _ou_ Position( sAprès, [ "[", "![" ] ) > Position( sAprès, "]" ) ALORS
      			bLibellé = vrai
      		fin
      	FIN
      	
      	si bLibellé alors
      	
      		pour toute sRemplacement, sTouche de Touches
      			si _eve.wparam = asc( sTouche ) ALORS
      				si ToucheEnfoncée( teshift ) = faux alors
      					
      					// Autorise ces caractères en début de ligne
      					si sTouche dans ( "#", "-", "`" ) ALORS
      						renvoyer faux
      					FIN
      				
      					SAI_MARKDOWN[[ nCurseur - 1 SUR 1 ]]			= sRemplacement
      					SAI_MARKDOWN.Curseur							= nCurseur + 1
      					SAI_MARKDOWN.FinCurseur							= nFinCurseur + 1
      					renvoyer vrai				
      				fin
      			FIN
      		FIN
      	fin
      	
      renvoyer faux
     type : 458752
   -
     name : __SurToucheEntrée
     procedure_id : 1402751578933867792
     type_code : 14
     code : |1-
      // Résumé : Appelé lors de l'appui sur [Entrée]
      // Syntaxe :
      // __SurToucheEntrée ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure PRIVÉE __SurToucheEntrée()
      
      	nCurseur, nFinCurseur	sont des entiers	= ( SAI_MARKDOWN.Curseur, SAI_MARKDOWN.FinCurseur )
      	bFinLigne				est un booléen		= SAI_MARKDOWN[[ nCurseur sur 2 ]] = rc _ou_ nCurseur > taille( SAI_MARKDOWN )
      
      	si bFinLigne ALORS
      		
      		nTailleDébut est un entier = 2
      		nDiffDebut est un entier	
      		nPosDébut est un entier = Position( SAI_MARKDOWN, rc, nCurseur - 1, DepuisFin )
      		si nPosDébut = 0 alors nPosDébut = 1
      		
      		si SAI_MARKDOWN[[ nPosDébut sur 2 ]] = RC alors nPosDébut += 2
      		tantque SAI_MARKDOWN[[ nPosDébut + nDiffDebut SUR 1 ]] = tab 
      			nDiffDebut ++
      			nTailleDébut ++
      		FIN
      
      		sIndice est une chaine		
      		bListeOrdonnée est un booléen = RegexVérifie( ExtraitChaîne( SAI_MARKDOWN[[ nPosDébut + nDiffDebut À ]], 1, rc ), "([0-9]+)\. .*", sIndice )
      		bTabulations est un booléen   = RegexVérifie( ExtraitChaîne( SAI_MARKDOWN[[ nPosDébut + nDiffDebut À ]], 1, rc ), "([[%TAB%]]+).*", sIndice )
      		
      		// Liste à puces ou ordonnée
      		si SAI_MARKDOWN[[ nPosDébut + nDiffDebut sur 2 ]] = "- " _ou_ SAI_MARKDOWN[[ nPosDébut + nDiffDebut SUR 2 ]] = "* " _OU_ SAI_MARKDOWN[[ nPosDébut + nDiffDebut SUR 2 ]] = "* " _ou_ bListeOrdonnée _ou_ bTabulations ALORS
      		
      			nPosFin est un entier = Position( SAI_MARKDOWN, rc, nPosDébut + nDiffDebut ) - 1
      			si nPosFin < 1 alors nPosFin = taille( SAI_MARKDOWN )
      			
      			nTailleDébut += taille( sIndice )
      			
      		
      			// Si on est sur une ligne vide
      			si taille( SAI_MARKDOWN[[ nPosDébut + nDiffDebut a nPosFin ]] ) = nTailleDébut ALORS
      				// Supprime la ligne vide
      				SAI_MARKDOWN[[ nPosDébut À nPosFin ]] = ""
      				SAI_MARKDOWN.Curseur		= nPosDébut + 1
      				SAI_MARKDOWN.FinCurseur		= nPosDébut
      				RENVOYER Faux
      			FIN
      		
      			// On est à la fin du texte
      			si nCurseur > taille( SAI_MARKDOWN ) ALORS
      				
      				si bListeOrdonnée ALORS
      					SAI_MARKDOWN += rc + ( val( sIndice ) + 1 ) + ". "
      				sinon
      					SAI_MARKDOWN += rc + SAI_MARKDOWN[[ nPosDébut SUR nTailleDébut ]]
      				FIN
      				
      				SAI_MARKDOWN.Curseur 	= taille( SAI_MARKDOWN ) + 1
      				SAI_MARKDOWN.FinCurseur = 0
      			
      			// On est dans le texte
      			sinon
      				
      				SI bListeOrdonnée alors nTailleDébut += 2 
      				
      				SI bListeOrdonnée ALORS
      					SAI_MARKDOWN[[ nCurseur SUR nTailleDébut ]] = RC + ( Val( sIndice ) + 1 ) + ". " + RC + ( Val( sIndice ) + 2 ) + ". "
      					nTailleDébut -= 2
      				SINON
      					SAI_MARKDOWN[[ nCurseur SUR nTailleDébut ]] = RC + SAI_MARKDOWN[[ nPosDébut SUR nTailleDébut ]] + RC
      				FIN
      				
      				SAI_MARKDOWN.Curseur							= nCurseur + 2 + nTailleDébut
      				SAI_MARKDOWN.FinCurseur							= nCurseur
      			FIN
      		
      			RENVOYER faux			
      		FIN			
      	FIN
      	
      	
      	
      renvoyer vrai
     type : 458752
   -
     name : Charge
     procedure_id : 1404662508634160011
     type_code : 14
     group : 4
     code : |1-
      // Résumé : Charge un fichier markdown
      // Syntaxe :
      //Charge (<sFichier> est chaîne)
      //
      // Paramètres :
      //	sFichier (chaîne ANSI) : Fichier à charger
      // Valeur de retour :
      // 	Aucune
      //
      procédure Charge( sFichier est une chaine )
      
      	si __MaFenInterne.AutoriseOuverture = faux ALORS
      		RETOUR
      	FIN
      
      	si fFichierExiste( sFichier ) = faux ALORS
      		ErreurDéclenche( 404, "Fichier invalide" )
      		retour
      	FIN
      	
      	si VérifieSauvegardeSiModifié() = faux ALORS
      		retour
      	FIN
      	
      	SAI_MARKDOWN.Extra.FichierEnCours = sFichier
      
      	// Affecte
      	sMarkdown est une chaine = fChargeTexte( sFichier )
      	Affecte( sMarkdown )
      	
      	SI __VoletAssocié <> Null ALORS
      		__VoletAssocié.Libellé = fExtraitChemin( sFichier, fFichier + fExtension )		
      	FIN
     type : 458752
   -
     name : Enregistre
     procedure_id : 1404663659685466965
     type_code : 14
     group : 4
     code : |1-
      // Résumé : Enregistre le markdown
      // Syntaxe :
      //[ <Résultat> = ] Enregistre (<sFichier> est chaîne)
      //
      // Paramètres :
      //	sFichier (chaîne ANSI) : Fichier à enregistrer
      // Valeur de retour :
      // 	booléen : Vrai si le markdown a été enregistré, Faux sinon
      //
      procédure Enregistre( local sFichier est une chaine )
      
      	si sFichier = "" alors
      				
      		// Ouvre le sélecteur de fichiers
      		sFichier = fSélecteur("", "", "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Documents Markdown (*.MD)" + TAB + "*.MD", "MD", fselCrée + fselExiste)
      		si sFichier = "" ALORS
      			renvoyer faux			
      		FIN		
      	fin
      	
      	// Sauvegarde
      	sMarkdown est une chaine = SAI_MARKDOWN
      	si fSauveTexte( sFichier, sMarkdown ) alors
      	
      		SAI_MARKDOWN.Extra.FichierEnCours	= sFichier
      		SAI_MARKDOWN.Modifié				= Faux
      	
      		SI __VoletAssocié <> Null ALORS
      			__VoletAssocié.Libellé = fExtraitChemin( sFichier, fFichier + fExtension )
      		fin
      	
      		renvoyer vrai	
      	fin
      	
      renvoyer faux
     type : 458752
