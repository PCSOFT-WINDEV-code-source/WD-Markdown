     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1-
      
      // Ferme les volets ouverts
      pour tout Volet, Alias de Editeurs	
      	SurFermetureVolet( Volet )
      	ONG_MD.Ferme( Alias )
      FIN
     type : 2
  procedures :
   -
     name : SurNouvelOnglet
     procedure_id : 1404885125407744512
     type_code : 14
     code : |1+
      // Résumé : Exécuté lors de l'ouverture d'un volet
      // Syntaxe :
      // SurNouvelOnglet ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure privee SurNouvelOnglet( Volet est un champ )
      
      	si ONG_MD.Occurrence > 0 ALORS
      		ONG_MD.Visible = vrai
      	FIN
      
      	// Mémorise
      	Editeurs[ Volet..Alias ] <- Volet
      
      	Volet.FI_EDITEUR.VoletAssocié = Volet		
     type : 458752
   -
     name : SurFermetureVolet
     procedure_id : 1404934264121341773
     type_code : 14
     code : |1-
      // Résumé : Déclenché lors de la fermeture d'un volet
      // Syntaxe :
      //SurFermetureVolet (<Volet> est champ)
      //
      // Paramètres :
      //	Volet (champ) : Volet
      // Valeur de retour :
      // 	Aucune
      //
      procédure PRIVÉE SurFermetureVolet( Volet est un champ )
      
      	// Informe l'éditeur de la fermeture
      	SI Volet <> Null ALORS
      		SI Volet.FI_EDITEUR.SurFermeture()	= Faux ALORS
      			DonneFocusEtRetourUtilisateur( FEN_MARKDOWN )
      		FIN
      	FIN
      	
      	// Mémorise
      	Supprime( Editeurs, Volet..Alias )
      	
      	SI ONG_MD.Occurrence <= 1 ALORS
      		ONG_MD.Visible = Faux
      	FIN
     type : 458752
