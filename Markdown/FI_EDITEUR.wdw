                  // En différé, supprime l'espace en fin de sélection
                  timersys( __SupprimeEspaceSélection, 20 ms )
                  	
                  procedure interne __SupprimeEspaceSélection() 
                  		
                  	fintimersys
                  	
                  	// Au double clic, si un sélection est effectuée, élimine les espaces en fin
                  	si SAI_MARKDOWN.TexteSélectionné =] " " ALORS
                  		SAI_MARKDOWN.FinCurseur --		
                  	FIN
                  	
                  	// Cas particulier pour la couleur
                  	si SAI_MARKDOWN[[ SAI_MARKDOWN.Curseur - 1 ]] = " " ALORS
                  		si RegexVérifie( "<span " + SAI_MARKDOWN.TexteSélectionné, "<span style=""color:[^""]+"">.*<\/span>" ) ALORS
                  			nFin est un entier = SAI_MARKDOWN.FinCurseur	
                  			SAI_MARKDOWN.Curseur -= 6
                  			SAI_MARKDOWN.FinCurseur = nFin
                  		FIN
                  	FIN
                  
                  fin
